#!/bin/bash
# Load all dotfiles

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

source "$DIR/.options"
source "$DIR/.complete"
source "$DIR/.exports"
source "$DIR/.functions"
source "$DIR/.aliases"
source "$DIR/.cheatsheets"
source "$DIR/.totvsfunctions"
source "$DIR/.priv8functions"

echo '~~ got r00t? ;-)'
